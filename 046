#include <stdio.h>
#include <stdlib.h>

int main (){
    int tabua,i, prod;

     printf ("Qual tabuada (1 a 10) voce gostaria de saber?:");
     scanf ("%d", &tabua);

     switch (tabua){
 case 1:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;
case 2:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 3:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 4:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 5:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 6:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 7:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 8:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 9:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

case 10:
    for (i=1; i<=10; i++){
        prod = tabua * i;
    printf ("%d x %d = %d \n", tabua, i, prod);
    }
   break;

default:
  printf ("opção invalida");
     }

#include <stdio.h>
#include <stdlib.h>

// categoria de idade

int main (){
  int idade;

    printf ("insira sua idade: ");
    scanf ("%d", &idade);

    if (idade < 12)
        printf ("CrianÃ§a");
    else if (idade >= 12 && idade <= 17)
        printf ("Adolescente");
    else if (idade >= 18 && idade <= 64)
        printf ("Adulto");
    else
        printf ("Idoso");

return 0;
}

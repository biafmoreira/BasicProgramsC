#include <stdio.h>
#include <stdlib.h>

int main (){

    int num1, num2, num3, num4, num5, positivos=0, negativos=0;

    printf ("insira cinco numeros: ");
    scanf ("%d\n%d\n%d\n%d\n%d", &num1, &num2, &num3, &num4, &num5);

    if (num1 < 0)
        negativos++;
    else
        positivos++;

     if (num2 < 0)
        negativos++;
    else
        positivos++;

     if (num3 < 0)
        negativos++;
    else
        positivos++;

     if (num4 < 0)
        negativos++;
    else
        positivos++;

     if (num5 < 0)
        negativos++;
    else
        positivos++;


        printf ("essa eh a quantidade de numeros negativos: %d \n essa eh a quantidade de numeros positivos: %d \n", negativos, positivos);

    return 0;
}
